{
  "name": "algorealm-ui",
  "description": "Claim the Crown and the Sceptre of Algorand Realm",
  "version": "0.3.0",
  "private": true,
  "author": "Cosimo Bassi <cosimo.bassi@algorand.com>",
  "license": "GPL-3.0",
  "keywords": [
    "nextjs",
    "algorealm",
    "algorand"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next build && next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest --watch",
    "test:ci": "jest --ci --collectCoverage",
    "test:e2e:ci": "playwright test",
    "test:e2e:local": "E2E_TESTS_BASE_URL=http://localhost:3000 playwright test --workers=1 --headed",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@fontsource/roboto": "4.5.8",
    "@hookform/resolvers": "2.9.11",
    "@mui/icons-material": "5.11.11",
    "@mui/lab": "5.0.0-alpha.123",
    "@mui/material": "5.11.13",
    "@mui/x-data-grid": "5.17.26",
    "@perawallet/connect": "^1.0.7",
    "@reduxjs/toolkit": "1.9.3",
    "algosdk": "2.9.0",
    "axios": "1.3.4",
    "csstype": "^3.1.1",
    "next": "13.2.4",
    "next-pwa": "5.6.0",
    "nextjs-google-analytics": "2.3.3",
    "notistack": "2.0.8",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-console-emulator": "^5.0.2",
    "react-dom": "18.2.0",
    "react-number-format": "4.9.4",
    "react-redux": "8.0.5",
    "react-tsparticles": "2.9.3",
    "react-use": "17.4.0",
    "swr": "1.3.0",
    "tsparticles": "2.9.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@playwright/test": "1.31.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "autoprefixer": "10.4.14",
    "cd": "0.3.3",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.0.3",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.0",
    "postcss": "8.4.21",
